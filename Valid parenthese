def isValid(s):
    stack = []
    pairs = {')': '(', '}': '{', ']': '['}

    for ch in s:
        # If it is a closing bracket
        if ch in pairs:
            if stack and stack[-1] == pairs[ch]:
                stack.pop()
            else:
                return False
        else:
            # Opening bracket
            stack.append(ch)

    return len(stack) == 0


# Test Cases
print(isValid("()"))        # True
print(isValid("()[]{}"))    # True
print(isValid("(]"))        # False
print(isValid("([])"))      # True
print(isValid("([)]"))      # False
