import math

def area(a, b, c):
    s = (a + b + c) / 2
    return math.sqrt(s * (s - a) * (s - b) * (s - c))

n = int(input())
triangles = []

for _ in range(n):
    a, b, c = map(int, input().split())
    triangles.append((a, b, c, area(a, b, c)))

# Sort by area (last element in the tuple)
triangles.sort(key=lambda x: x[3])

# Print only sides
for t in triangles:
    print(t[0], t[1], t[2])
